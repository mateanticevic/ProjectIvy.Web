{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "arrow-parens": [
            "as-needed"
        ],
        "interface-name": [
            true,
            "never-prefix"
        ],
        "max-line-length": [
            true,
            250
        ],
        "quotemark": [
            true,
            "single",
            "jsx-double"
        ]
    },
    "rulesDirectory": []
}